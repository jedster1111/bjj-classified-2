{
  "compilerOptions": {
      "declaration": true,
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "composite": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "declarationMap": true,
      "noImplicitOverride": true
  }
}
