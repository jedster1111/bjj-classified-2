{
  "parser":  "@typescript-eslint/parser",  
  "extends":  [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions":  {
    "ecmaVersion":  2020,  
    "sourceType":  "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules":  {
    "prettier/prettier": ["error", {
     "endOfLine":"auto"
   }],
   "@typescript-eslint/explicit-module-boundary-types": 0
  }
}
